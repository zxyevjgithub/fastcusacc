       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="config" class="com.cache.Cachefresh" ></bean>

    <bean id="factorWeMap" class="java.util.HashMap">
        <constructor-arg>
            <map>
                <entry key="cash" value="12"></entry>
            </map>
        </constructor-arg>
    </bean>

    <bean id="handler" class="com.biz.AccountProcessHandler" >
    </bean>
</beans>